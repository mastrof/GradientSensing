@article{seymour2010chemoattraction,
  title = {Chemoattraction to {{Dimethylsulfoniopropionate Throughout}} the {{Marine Microbial Food Web}}},
  author = {Seymour, Justin R. and Sim{\'o}, Rafel and Ahmed, Tanvir and Stocker, Roman},
  year = {2010},
