@article{wosniack2022adaptation,
  title = {Adaptation of {{Drosophila}} Larva Foraging in Response to Changes in Food Resources},
  author = {Wosniack, Marina E and Festa, Dylan and Hu, Nan and Gjorgjieva, Julijana and Berni, Jimena},
  year = {2022},
