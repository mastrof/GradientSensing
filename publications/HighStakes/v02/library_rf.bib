@article{kringle2021structural,
  title = {Structural Relaxation and Crystallization in Supercooled Water from 170 to 260 {{K}}},
  author = {Kringle, Loni and Thornley, Wyatt A. and Kay, Bruce D. and Kimmel, Greg A.},
  year = {2021},
