@article{kiorboe2002mechanisms,
  title = {Mechanisms and {{Rates}} of {{Bacterial Colonization}} of {{Sinking Aggregates}}},
  author = {Ki{\o}rboe, Thomas and Grossart, Hans-Peter and Ploug, Helle and Tang, Kam},
  year = {2002},
  month = aug,
  journal = {Appl. Environ. Microbiol.},
  volume = {68},
  number = {8},
  pages = {3996--4006},
