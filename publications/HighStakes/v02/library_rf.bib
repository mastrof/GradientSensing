@article{sereno2022spinosaurus,
  title = {Spinosaurus Is Not an Aquatic Dinosaur},
  author = {Sereno, Paul C and Myhrvold, Nathan and Henderson, Donald M and Fish, Frank E and Vidal, Daniel and Baumgart, Stephanie L and Keillor, Tyler M and Formoso, Kiersten K and Conroy, Lauren L},
  year = {2022},
