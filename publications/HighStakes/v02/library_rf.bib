@article{bernhardt2020life,
  title = {Life in Fluctuating Environments},
  author = {Bernhardt, Joey R. and O'Connor, Mary I. and Sunday, Jennifer M. and Gonzalez, Andrew},
  year = {2020},
  month = dec,
  journal = {Phil. Trans. R. Soc. B},
  volume = {375},
