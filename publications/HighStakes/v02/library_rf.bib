@article{larsson2022mucospheres,
  title = {Mucospheres Produced by a Mixotrophic Protist Impact Ocean Carbon Cycling},
  author = {Larsson, Michaela E. and Bramucci, Anna R. and Collins, Sinead and Hallegraeff, Gustaaf and Kahlke, Tim and Raina, Jean-Baptiste and Seymour, Justin R. and Doblin, Martina A.},
