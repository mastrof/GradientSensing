@article{slomka2023encounter,
  title = {Encounter Rates Prime Interactions between Microorganisms},
  author = {S{\l}omka, Jonasz and Alcolombri, Uria and Carrara, Francesco and Foffi, Riccardo and Peaudecerf, Fran{\c c}ois J. and Zbinden, Matti and Stocker, Roman},
  year = {2023},
