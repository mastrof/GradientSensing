  urldate = {2023-01-08},
  abstract = {Crystalline Cu               2               Te has recently attracted a great deal of attention owing to its good performance in thermoelectric materials. Yet, knowledge of the amorphous phase is still insufficient, which may restrict its practical application. Here, we have studied the structural and bonding characteristics of amorphous Cu               2               Te by ab initio molecular dynamics simulations. We show that, compared with its crystal phase, the Cu atoms bond more Cu than Te atoms in amorphous Cu               2               Te and Te atoms predominantly bond with Cu atoms. In detail, the amorphous Cu               2               Te is made up of Cu{\textendash}Te network structures and Cu{\textendash}Cu high-coordinated configurations, presenting the hexagonal and icosahedral structures, respectively. This result is probably ascribed to both the stronger bonding ability of Cu{\textendash}Cu bonds and the multivalence of Te atoms. Our findings enrich the knowledge of the local structure and the bonding nature of amorphous Cu               2               Te, which can guide the design of good performance Cu               2               Te based thermoelectric devices further.},
  langid = {english},
  file = {/home/riccardo/Zotero/storage/VDK7ULRA/gao_2022_the_changeable_coordination_of.pdf}
}

@article{gartner2020signatures,
  title = {Signatures of a Liquid{\textendash}Liquid Transition in an Ab Initio Deep Neural Network Model for Water},
  author = {Gartner, Thomas E. and Zhang, Linfeng and Piaggi, Pablo M. and Car, Roberto and Panagiotopoulos, Athanassios Z. and Debenedetti, Pablo G.},
  year = {2020},
  month = oct,
  journal = {Proc. Natl. Acad. Sci. USA},
  pages = {202015440},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.2015440117},
  urldate = {2020-10-12},
  abstract = {The possible existence of a metastable liquid{\textendash}liquid transition (LLT) and a corresponding liquid{\textendash}liquid critical point (LLCP) in supercooled liquid water remains a topic of much debate. An LLT has been rigorously proved in three empirically parametrized molecular models of water, and evidence consistent with an LLT has been reported for several other such models. In contrast, experimental proof of this phenomenon has been elusive due to rapid ice nucleation under deeply supercooled conditions. In this work, we combined density functional theory (DFT), machine learning, and molecular simulations to shed additional light on the possible existence of an LLT in water. We trained a deep neural network (DNN) model to represent the ab initio potential energy surface of water from DFT calculations using the Strongly Constrained and Appropriately Normed (SCAN) functional. We then used advanced sampling simulations in the multithermal{\textendash}multibaric ensemble to efficiently explore the thermophysical properties of the DNN model. The simulation results are consistent with the existence of an LLCP, although they do not constitute a rigorous proof thereof. We fit the simulation data to a two-state equation of state to provide an estimate of the LLCP's location. These combined results{\textemdash}obtained from a purely first-principles approach with no empirical parameters{\textemdash}are strongly suggestive of the existence of an LLT, bolstering the hypothesis that water can separate into two distinct liquid forms.},
  langid = {english},
  file = {/home/riccardo/Zotero/storage/P3XECJNV/gartner_2020_signatures_of_a_liquid‚Äìliquid.pdf}
}

@article{gartner2021manifestations,
